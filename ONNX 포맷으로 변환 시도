torch.onnx.export(
    model,
    dummy_input,  # Use the tensor directly
    "logodetection_model.onnx",
    export_params=True,
    opset_version=11,
    do_constant_folding=True,
    input_names=['image_tensor'],
    output_names=['output'],
    dynamic_axes={'image_tensor': {0: 'batch_size'},
                  'output': {0: 'num_boxes'}}
)